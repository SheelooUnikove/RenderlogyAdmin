    <div class="container">

        <div class="row">

            <div class="col-xs-12 col-sm-12 col-lg-12">

                <div class="row">
                    <div class="col-12 col-sm-12 col-lg-12 align-center">
                        <h1>Search for a Contact</h1>
                    </div>
                    <div class="col-12 col-sm-12 col-lg-12 align-center">
                        @Html.ValidationSummary(true)
                    </div>
                    @using (Html.BeginForm("SeacrhContact", "CRM", FormMethod.Post))
                    {
                        <div class="col-6 col-sm-6 col-lg-6">
                            <div class="form-group">
                                <label class="form-label"><strong>Contact Name</strong> </label>
                                <div class="controls">
                                    @Html.TextBoxFor(model => model.ContactName, new { @class = "form-control input-sm", placeholder = "Contact Name" })
                                </div>
                            </div>
                            <div class="form-group">
                                <label class="form-label"><strong>City</strong> </label>
                                @Html.DropDownListFor(model => model.ProjectLocationId, new SelectList(ViewBag.CityList, "CityID", "CityName"), new { @class = "selectpicker show-tick form-control", @data_live_search = "true" })
                            </div>
                            <div class="form-group">
                                <label class="form-label"><strong>Status</strong> </label>
                                @Html.DropDownListFor(model => model.AccountTypeId, new SelectList(ViewBag.AccountTypeMasterList, "AccountTypeId", "AccountTypeName"), new { @class = "selectpicker show-tick form-control", @data_live_search = "true" })
                            </div>
                        </div>
                        <!--/span-->
                        <div class="col-6 col-sm-6 col-lg-6">
                            <div class="form-group m-b-12">
                                <label class="form-label"><strong>Developer</strong> </label>
                                @Html.DropDownListFor(model => model.DeveloperId, new SelectList(ViewBag.IndexDeveloperMasterList, "DeveloperId", "DeveloperName"), new { @class = "selectpicker show-tick form-control", @data_live_search = "true" })
                            </div>
                            <div class="form-group m-b-15">
                                <label class="form-label"><strong>Account Manager</strong> </label>
                                @Html.DropDownListFor(model => model.AccountManagerId, new SelectList(ViewBag.AccountManagerMasterList, "AccountManagerId", "AccountManagerName") ,"Select",new { @class = "selectpicker show-tick form-control", @data_live_search = "true" })
                            </div>
                            <div class="form-group m-b-0">
                                <label class="form-label"><strong>Creation Date</strong> </label>
                            </div>
                            <div class="form-group">
                                <div class="controls col-sm-5 col-md-5 col-lg-5 p-0">
                                    @Html.TextBoxFor(model => model.CreationDate, new { @class = "pickadate form-control picker__input", placeholder = "Click me to see calendar" })
                                </div>
                                <div class="controls col-sm-2 col-md-2 col-lg-2 align-center">to </div>
                                <div class="controls col-sm-5 col-md-5 col-lg-5 p-0">
                                    @Html.TextBoxFor(model => model.CreationDateEnd, new { @class = "pickadate form-control picker__input", placeholder = "Click me to see calendar" })
                                </div>
                            </div>
                        </div>
                        <div class="clearfix"></div>

                        <div class="col-xs-6 col-sm-6 col-lg-6 align-right m-t-20">

                            <button class="btn btn-primary" id="SeacrhContact" name="SeacrhContact" value="SeacrhContact">Search</button>

                        </div> 
                    }
                    <div class="col-xs-6 col-sm-6 col-lg-6 m-t-20">
                        @using (Html.BeginForm("IndexClear", "CRM", FormMethod.Post))
                        {
                            <button class="btn btn-dark" id="IndexClear" name="IndexClear" value="IndexClear">Cancel</button>
                        }
                    </div>
                    <!--/span-->
                    <!--/span-->
                    <!--/span-->
                </div>

                <!--/row-->
            </div>

            <!--/span-->

            <!--/span-->
        </div>
        <!--/row-->

    </div> 
        
    <div class="container-fluid bg-gray-light m-t-30">
        <div class="container m-t-30">

            <div class="row">
                <div class="col-md-12">
                    <div class="panel panel-default">
                        <div class="panel-heading bg-gray">
                            <h3 class="panel-title width_auto"><strong>All Contacts </strong></h3>
                            <div class="flt_rht">
                                <span class="btn btn-default fileinput-button btn-sm">
                                    <span>Upload Contact</span>
                                    <input id="fileupload" type="file" name="fileupload" accept=".csv, application/vnd.openxmlformats-officedocument.spreadsheetml.sheet, application/vnd.ms-excel">
                                </span>
                                @using (Html.BeginForm("ExportData", "CRM", FormMethod.Post, new { @class = "flt_lft", enctype = "multipart/form-data" }))
                                {              
                                    <input type="submit" name="ExportData" id="ExportData" value="ExportData" class="btn btn-default btn-sm  flt_lft m-r-20" />                                     
                                }
                                @Html.ActionLink("Add Contact", "Create", null, new { @class = "btn btn-sm  btn-info flt_lft m-r-20" })
                            </div>
                        </div>
                        <div class="panel-body">
                            <div class="row">
                                <div class="col-md-12 col-sm-12 col-xs-12 table-responsive filter-right">
                                    <table class="table table-hover table-dynamic">
                                        <thead>
                                            <tr>
                                                <th>Contact Name</th>
                                                <th>Developer</th>
                                                <th>City</th>
                                                <th class='hidden-350'>Account Manager</th>
                                                <th class='hidden-1024'>Status</th>
                                                <th class='hidden-480'>Creation Date</th>
                                                 <th class='hidden-480'>Send Invitation</th>
                                            </tr>
                                        </thead>
                                        <tbody>
                                            @foreach (var item in ViewBag.CRMContactList)
                                            {                                             
                                                <tr>
                                                    <td>@Html.ActionLink((string)@item.ContactName, "ViewContact", new { id = @item.ContactId })</td>
                                                    <td>@Html.ActionLink((string)@item.DeveloperName, "ViewContact", new { id = @item.ContactId })</td>
                                                    <td>@Html.ActionLink((string)@item.ProjectLocationCity, "ViewContact", new { id = @item.ContactId })</td>
                                                    <td class='hidden-350'>@Html.ActionLink((string)@item.AccountManagerName, "ViewContact", new { id = @item.ContactId })</td>
                                                    <td class='hidden-1024'>@Html.ActionLink((string)@item.ContactStatusName, "ViewContact", new { id = @item.ContactId })</td>
                                                    <td class='hidden-480'>
                                                        @if (!string.IsNullOrEmpty(@item.CreationDate.ToString()))
                                                        {
                                                            
                                                            @Html.ActionLink((string)Convert.ToDateTime(@item.CreationDate).ToString("dd MMM, yyyy"), "ViewContact", new { id = @item.ContactId })
                                                        }
                                                    </td>
                                                    <td> 
                                                        @if(@item.IsActive==true)
                                                        {                                                    
                                                       
                                                        }
                                                        else
                                                        {
                                                         @Html.ActionLink("Send", "SendInvitationtoUser", new { id = @item.ContactId })
                                                        }
                                                    </td>
                                                </tr>  
                                            }
                                        </tbody>
                                    </table>
                                </div>
                            </div>
                        </div>

                    </div>
                </div>
            </div>
        </div>
    </div>
