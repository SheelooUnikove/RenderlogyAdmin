    <!-- BEGIN contact information -->

    <div class="tabcordion">
        <div class="container-fluid bg-gray-light p-t-20">
            <div class="container">
                <div class="row">
                    <div class="col-xs-5 col-sm-5 col-md-5 col-lg-5"></div>
                    <div>
                        <ul id="myTab" class="nav nav-tabs">
                            <li class="active"><a href="#tab1_1" data-toggle="tab">Contact Information</a></li>
                            <li class=""><a href="#tab1_2" data-toggle="tab">History</a></li>

                        </ul>
                    </div>
                    <div class="col-xs-5 col-sm-5 col-md-5 col-lg-5"></div>
                </div>
            </div>
        </div>
        <div class="container">
            <div class="row">
                <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
                    <div class="tab-content">
                        <div class="tab-pane fade active in" id="tab1_1">
                            <div class="col-xs-12 col-sm-12 col-lg-12">
                                <div class="row">
                                    <div class="col-12 col-sm-12 col-lg-12 align-center">
                                        <h1>Contact Information</h1>
                                    </div>
                                    <div class="row">
                                        <div class="col-6 col-sm-6 col-lg-6">
                                            <div class="form-group">
                                                <label class="form-label"><strong>Contact Name</strong> </label>
                                                <div class="control">
                                                    @Html.HiddenFor(model => model.ContactId)
                                                    @Html.TextBoxFor(model => model.ContactName, new { @class = "form-control input-sm", disabled = true })

                                                </div>
                                            </div>
                                        </div>
                                        <div class="col-6 col-sm-6 col-lg-6">
                                            <div class="form-group">
                                                <label class="form-label"><strong>Account Type</strong> </label>
                                                <div class="control">
                                                    @Html.TextBoxFor(model => model.AccountTypeName, new { @class = "form-control input-sm", disabled = true })
                                                </div>
                                            </div>
                                        </div>
                                        <div class="clearfix"></div>
                                        <div class="col-6 col-sm-6 col-lg-6">
                                            <div class="form-group">
                                                <label class="form-label"><strong>Account Name</strong> </label>
                                                <div class="control">
                                                    @Html.TextBoxFor(model => model.AccountMasterName, new { @class = "form-control input-sm", disabled = true })
                                                    @Html.TextBoxFor(model => model.OtherAccountName, new { @class = "form-control input-sm", disabled = true })
                                                </div>
                                            </div>
                                        </div>
                                        <div class="col-6 col-sm-6 col-lg-6">
                                            <div class="form-group">
                                                <label class="form-label"><strong>Developer</strong> </label>
                                                <div class="control">
                                                    @Html.TextBoxFor(model => model.DeveloperName, new { @class = "form-control input-sm", disabled = true })
                                                    @Html.TextBoxFor(model => model.OtherDeveloperName, new { @class = "form-control input-sm", disabled = true })
                                                </div>
                                            </div>
                                        </div>
                                        <div class="clearfix"></div>
                                        <div class="col-6 col-sm-6 col-lg-6">
                                            <div class="form-group m-b-12">
                                                <label class="form-label"><strong>City</strong> </label>
                                                <div class="control">
                                                    @Html.TextBoxFor(model => model.ProjectLocationCity, new { @class = "form-control input-sm", disabled = true })
                                                </div>
                                            </div>
                                        </div>
                                        <div class="col-6 col-sm-6 col-lg-6">
                                            <div class="form-group">
                                                <label class="form-label"><strong>Address</strong> </label>
                                                <div class="control">
                                                    @Html.TextBoxFor(model => model.Address, new { @class = "form-control input-sm", disabled = true })
                                                </div>
                                            </div>
                                        </div>
                                        <div class="clearfix"></div>
                                        <div class="col-6 col-sm-6 col-lg-6">
                                            <div class="form-group m-b-15">
                                                <label class="form-label"><strong>Mobile Number</strong> </label>
                                                <div class="control">
                                                    @Html.TextBoxFor(model => model.MobileNumber, new { @class = "form-control input-sm", disabled = true })
                                                </div>
                                            </div>
                                        </div>
                                        <div class="col-6 col-sm-6 col-lg-6">
                                            <div class="form-group">
                                                <label class="form-label"><strong>Phone Number</strong> </label>
                                                <div class="control">
                                                    @Html.TextBoxFor(model => model.PhoneNumber, new { @class = "form-control input-sm", disabled = true })
                                                </div>
                                            </div>
                                        </div>
                                        <div class="clearfix"></div>
                                        <div class="col-6 col-sm-6 col-lg-6">
                                            <div class="form-group m-b-12">
                                                <label class="form-label"><strong>Email Address</strong> </label>
                                                <div class="control">
                                                    @Html.TextBoxFor(model => model.ContactEmailAddress, new { @class = "form-control input-sm", disabled = true })
                                                </div>
                                            </div>
                                        </div>
                                        <div class="col-6 col-sm-6 col-lg-6">
                                            <div class="form-group">
                                                <label class="form-label"><strong>Account Manager</strong> </label>                                                
                                                <div class="control">
                                                    @Html.TextBoxFor(model => model.AccountManagerName, new { @class = "form-control input-sm", disabled = true })
                                                </div>
                                            </div>
                                        </div>
                                        <div class="clearfix"></div>
                                        <div class="col-6 col-sm-6 col-lg-6">
                                            <div class="form-group">
                                                <label class="form-label"><strong>Status</strong> </label>
                                                <div class="control">
                                                    @Html.TextBoxFor(model => model.ContactStatusName, new { @class = "form-control input-sm", disabled = true })
                                                </div>
                                            </div>
                                        </div>
                                        <div class="col-6 col-sm-6 col-lg-6">
                                            <div class="form-group">
                                                <label class="form-label"><strong>Budget</strong> </label>
                                                <div class="control">
                                                    @Html.TextBoxFor(model => model.Budget, new { @class = "form-control input-sm", disabled = true })

                                                </div>
                                            </div>
                                        </div>
                                        <div class="clearfix"></div>
                                        <div class="col-6 col-sm-6 col-lg-6">
                                            <div class="form-group m-b-12">
                                                <label class="form-label"><strong>Referal Source</strong> </label>
                                                <div class="control">
                                                    @Html.TextBoxFor(model => model.ReferralSourceName, new { @class = "form-control input-sm", disabled = true })
                                                </div>
                                            </div>
                                        </div>

                                        <div class="col-6 col-sm-6 col-lg-6" style="display: none;">
                                            <div class="form-group">
                                                <label class="form-label"><strong>Type of conversation</strong> </label>
                                                <div class="control">
                                                    @Html.TextBoxFor(model => model.TypeofconversationName, new { @class = "form-control input-sm", disabled = true })
                                                </div>
                                            </div>
                                        </div>
                                        <div class="clearfix"></div>
                                    </div>
                                    <div class="clearfix"></div>
                                    <div class="col-xs-6 col-sm-6 col-md-6  col-lg-6 align-right m-t-20 m-b-30">
                                        @Html.ActionLink("Back to List", "Index", null, new { @class = "btn btn-primary" })
                                    </div>
                                    <div class="col-xs-6 col-sm-6 col-md-6  col-lg-6 m-t-20 align-left">
                                        @Html.ActionLink("Edit Information", "Edit", new { id = Model.ContactId }, new { @class = "btn btn-dark" })
                                    </div>
                                    <!--/span-->
                                    <!--/span-->
                                    <!--/span-->
                                </div>
                                <!--/row-->
                            </div>
                        </div>
                        <div class="tab-pane fade" id="tab1_2">
                            <div class="container m-t-30">

                                <div class="row">
                                    <div class="col-md-12">
                                        <div class="panel panel-default">
                                            <div class="panel-heading bg-gray">
                                                <h3 class="panel-title"><strong>History </strong></h3>
                                            </div>
                                            <div class="panel-body">
                                                <div class="row m-0">
                                                    <div class="col-md-12 col-sm-12 col-xs-12 table-responsive filter-right">



                                                        <table class="table table-hover table-dynamic">
                                                            <thead>
                                                                <tr>
                                                                    <th>Event Date</th>
                                                                    <th>Time</th>
                                                                    <th class='hidden-350'>Owner</th>
                                                                    <th class='hidden-1024'>Event</th>
                                                                    <th class='hidden-480'>Call Recording Date</th>
                                                                </tr>
                                                            </thead>
                                                            <tbody>
                                                                @foreach (var item in ViewBag.FollowAlerts)
                                                                {
                                                                    <tr>
                                                                        <td>
                                                                            @Html.ActionLink((string)@Convert.ToDateTime(@item.FollowUpDate).ToString("dd MMM, yyyy"), "UpdateHistory", new { id = @item.AlertId })
                                                                        </td>
                                                                        <td>@Html.ActionLink((string)@item.FollowUpHH, "UpdateHistory", new { id = @item.AlertId }) </td>
                                                                        @*: (string)@item.FollowUpMM (string)@item.FollowUpAMPM*@
                                                                        <td class='hidden-350'>Bob Nilson</td>
                                                                        <td class='hidden-1024'>@Html.ActionLink((string)@item.TypeofconversationName, "UpdateHistory", new { id = @item.AlertId })</td>
                                                                        <td class='hidden-480'></td>
                                                                    </tr>                                                             
                                                                }
                                                            </tbody>
                                                        </table>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>


                            </div>
                        </div>


                    </div>
                </div>
            </div>
        </div>

    </div>






    <!-- BEGIN MANDATORY SCRIPTS -->
