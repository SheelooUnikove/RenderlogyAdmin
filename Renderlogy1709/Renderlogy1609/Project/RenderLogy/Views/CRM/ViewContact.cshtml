    <script src="~/assets/js/application.js"></script>

    <script type="text/javascript">
        $(window).on('load', function () {

            if ($("#OtherAccountName").val() == "")
                $("#OtherAccountName").hide();
            else
                $("#OtherDeveloperName").show();


            if ($("#OtherDeveloperName").val() == "")
                $("#OtherDeveloperName").hide();
            else
                $("#OtherDeveloperName").show();

        });
    </script>
</body>
</html>
